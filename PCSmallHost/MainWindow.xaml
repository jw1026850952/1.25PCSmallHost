    <Window.Resources>
        <Style x:Key="PasswordKeyboardStyle" TargetType="Button">
            <Setter Property="Background" Value="Transparent" />
            <Setter Property="VerticalAlignment" Value="Center" />
            <Setter Property="Padding" Value="0" />
            <Setter Property="FontSize" Value="30" />
            <Setter Property="Template">
                <Setter.Value>
                    <ControlTemplate TargetType="Button">
                        <Grid>
                            <Image
                                Width="70"
                                Height="70"
                                Source="/Pictures/NumButton.png" />
                            <Label
                                Width="60"
                                Height="60"
                                HorizontalContentAlignment="Center"
                                VerticalContentAlignment="Center"
                                Content="{TemplateBinding Content}"
                                FontSize="{TemplateBinding FontSize}"
                                Foreground="#FFFFFF" />
                        </Grid>
                    </ControlTemplate>
                </Setter.Value>
            </Setter>
        </Style>

        <!--进度条-->
        <Style x:Key="CustomProgressBarStyle" TargetType="ProgressBar">
            <Setter Property="Foreground" Value="#007ACC" />
            <Setter Property="Background" Value="#D6D6D6" />
            <Setter Property="BorderBrush" Value="#D6D6D6" />
            <Setter Property="BorderThickness" Value="1" />
            <Setter Property="Height" Value="30" />
            <Setter Property="Template">
                <Setter.Value>
                    <ControlTemplate TargetType="ProgressBar">
                        <Grid>
                            <Rectangle x:Name="PART_Track" Fill="{TemplateBinding Background}" />
                            <Rectangle x:Name="PART_Indicator" Fill="{TemplateBinding Foreground}" />
                            <TextBlock x:Name="PercentageText" Text="{Binding ImportedFilesPercentage, StringFormat={}{0}%}" Foreground="White" FontWeight="Bold" FontSize="14" HorizontalAlignment="Center" VerticalAlignment="Center" />
                            <TextBlock Text="主机数据下传中" Foreground="White" FontWeight="Bold" FontSize="14" Margin="5,0,0,0" />
                        </Grid>
                        <ControlTemplate.Triggers>
                            <Trigger Property="Orientation" Value="Vertical">
                                <Setter TargetName="PercentageText" Property="Text" Value="{Binding ImportedFilesPercentage, StringFormat={}{0}%}" />
                            </Trigger>
                        </ControlTemplate.Triggers>
                    </ControlTemplate>
                </Setter.Value>
            </Setter>
        </Style>
    </Window.Resources>

